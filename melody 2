const int pump=10;
const int solenoid_air_control=9;
const int solenoid_1=8;
const int solenoid_2=7;
const int solenoid_3=6;
const int solenoid_4=5;
const int solenoid_5=4;
const int solenoid_6=3;
const int solenoid_7=2;

 
 

void setup() {
  pinMode(pump,OUTPUT);
  pinMode(solenoid_air_control,OUTPUT);
  pinMode(solenoid_1,OUTPUT);
  pinMode(solenoid_2,OUTPUT);
  pinMode(solenoid_3,OUTPUT);
  pinMode(solenoid_4,OUTPUT);
  pinMode(solenoid_5,OUTPUT);
  pinMode(solenoid_6,OUTPUT);
  pinMode(solenoid_7,OUTPUT);
}



void loop() {
  digitalWrite(pump,HIGH);
  delay(2000);
  digitalWrite(pump,LOW);
  delay(2000);
  
  sol();
  delay(2000);
  digitalWrite(solenoid_air_control,HIGH);
  delay(1000);
  digitalWrite(solenoid_air_control,LOW);
  
  sol();
  delay(2000);
  digitalWrite(solenoid_air_control,HIGH);
  delay(1000);
  digitalWrite(solenoid_air_control,LOW);

  sol();
  delay(2000);
  digitalWrite(solenoid_air_control,HIGH);
  delay(1000);
  digitalWrite(solenoid_air_control,LOW);

  la();
  delay(2000);
  digitalWrite(solenoid_air_control,HIGH);
  delay(1000);
  digitalWrite(solenoid_air_control,LOW);

  si();
  delay(4000);
  digitalWrite(solenoid_air_control,HIGH);
  delay(1000);
  digitalWrite(solenoid_air_control,LOW);

  la();
  delay(4000);
  digitalWrite(solenoid_air_control,HIGH);
  delay(1000);
  digitalWrite(solenoid_air_control,LOW);

  sol();
  delay(2000);
  digitalWrite(solenoid_air_control,HIGH);
  delay(1000);
  digitalWrite(solenoid_air_control,LOW);

  si();
  delay(2000);
  digitalWrite(solenoid_air_control,HIGH);
  delay(1000);
  digitalWrite(solenoid_air_control,LOW);

  la();
  delay(2000);
  digitalWrite(solenoid_air_control,HIGH);
  delay(1000);
  digitalWrite(solenoid_air_control,LOW);
  
  la();
  delay(2000);
  digitalWrite(solenoid_air_control,HIGH);
  delay(1000);
  digitalWrite(solenoid_air_control,LOW);

  sol();
  delay(8000);
  digitalWrite(solenoid_air_control,HIGH);
  delay(1000);
  digitalWrite(solenoid_air_control,LOW);

/*première ligne*/

  sol();
  delay(2000);
  digitalWrite(solenoid_air_control,HIGH);
  delay(1000);
  digitalWrite(solenoid_air_control,LOW);
  
  sol();
  delay(2000);
  digitalWrite(solenoid_air_control,HIGH);
  delay(1000);
  digitalWrite(solenoid_air_control,LOW);

  sol();
  delay(2000);
  digitalWrite(solenoid_air_control,HIGH);
  delay(1000);
  digitalWrite(solenoid_air_control,LOW);

  la();
  delay(2000);
  digitalWrite(solenoid_air_control,HIGH);
  delay(1000);
  digitalWrite(solenoid_air_control,LOW);

  si();
  delay(4000);
  digitalWrite(solenoid_air_control,HIGH);
  delay(1000);
  digitalWrite(solenoid_air_control,LOW);

  la();
  delay(4000);
  digitalWrite(solenoid_air_control,HIGH);
  delay(1000);
  digitalWrite(solenoid_air_control,LOW);

  sol();
  delay(2000);
  digitalWrite(solenoid_air_control,HIGH);
  delay(1000);
  digitalWrite(solenoid_air_control,LOW);

  si();
  delay(2000);
  digitalWrite(solenoid_air_control,HIGH);
  delay(1000);
  digitalWrite(solenoid_air_control,LOW);

  la();
  delay(2000);
  digitalWrite(solenoid_air_control,HIGH);
  delay(1000);
  digitalWrite(solenoid_air_control,LOW);
  
  la();
  delay(2000);
  digitalWrite(solenoid_air_control,HIGH);
  delay(1000);
  digitalWrite(solenoid_air_control,LOW);

  sol();
  delay(8000);
  digitalWrite(solenoid_air_control,HIGH);
  delay(1000);
  digitalWrite(solenoid_air_control,LOW);
/*deuxième ligne*/

  la();
  delay(2000);
  digitalWrite(solenoid_air_control,HIGH);
  delay(1000);
  digitalWrite(solenoid_air_control,LOW);

  la();
  delay(2000);
  digitalWrite(solenoid_air_control,HIGH);
  delay(1000);
  digitalWrite(solenoid_air_control,LOW);


  la();
  delay(2000);
  digitalWrite(solenoid_air_control,HIGH);
  delay(1000);
  digitalWrite(solenoid_air_control,LOW);

  la();
  delay(2000);
  digitalWrite(solenoid_air_control,HIGH);
  delay(1000);
  digitalWrite(solenoid_air_control,LOW);

  mi();
  delay(4000);
  digitalWrite(solenoid_air_control,HIGH);
  delay(1000);
  digitalWrite(solenoid_air_control,LOW);

  mi();
  delay(4000);
  digitalWrite(solenoid_air_control,HIGH);
  delay(1000);
  digitalWrite(solenoid_air_control,LOW);

  la();
  delay(2000);
  digitalWrite(solenoid_air_control,HIGH);
  delay(1000);
  digitalWrite(solenoid_air_control,LOW);

  sol();
  delay(2000);
  digitalWrite(solenoid_air_control,HIGH);
  delay(1000);
  digitalWrite(solenoid_air_control,LOW);

  fa-d();
  delay(2000);
  digitalWrite(solenoid_air_control,HIGH);
  delay(1000);
  digitalWrite(solenoid_air_control,LOW);
  
  mi();
  delay(2000);
  digitalWrite(solenoid_air_control,HIGH);
  delay(1000);
  digitalWrite(solenoid_air_control,LOW);

  re();
  delay(8000);
  digitalWrite(solenoid_air_control,HIGH);
  delay(1000);
  digitalWrite(solenoid_air_control,LOW);

/*troisième ligne*/

  sol();
  delay(2000);
  digitalWrite(solenoid_air_control,HIGH);
  delay(1000);
  digitalWrite(solenoid_air_control,LOW);
  
  sol();
  delay(2000);
  digitalWrite(solenoid_air_control,HIGH);
  delay(1000);
  digitalWrite(solenoid_air_control,LOW);

  sol();
  delay(2000);
  digitalWrite(solenoid_air_control,HIGH);
  delay(1000);
  digitalWrite(solenoid_air_control,LOW);

  la();
  delay(2000);
  digitalWrite(solenoid_air_control,HIGH);
  delay(1000);
  digitalWrite(solenoid_air_control,LOW);

  si();
  delay(4000);
  digitalWrite(solenoid_air_control,HIGH);
  delay(1000);
  digitalWrite(solenoid_air_control,LOW);

  la();
  delay(4000);
  digitalWrite(solenoid_air_control,HIGH);
  delay(1000);
  digitalWrite(solenoid_air_control,LOW);

  sol();
  delay(2000);
  digitalWrite(solenoid_air_control,HIGH);
  delay(1000);
  digitalWrite(solenoid_air_control,LOW);

  si();
  delay(2000);
  digitalWrite(solenoid_air_control,HIGH);
  delay(1000);
  digitalWrite(solenoid_air_control,LOW);

  la();
  delay(2000);
  digitalWrite(solenoid_air_control,HIGH);
  delay(1000);
  digitalWrite(solenoid_air_control,LOW);
  
  la();
  delay(2000);
  digitalWrite(solenoid_air_control,HIGH);
  delay(1000);
  digitalWrite(solenoid_air_control,LOW);

  sol();
  delay(8000);
  digitalWrite(solenoid_air_control,HIGH);
  delay(1000);
  digitalWrite(solenoid_air_control,LOW);

}


void Dograve()
{
  digitalWrite(solenoid_1,HIGH);
  digitalWrite(solenoid_2,HIGH);
  digitalWrite(solenoid_3,HIGH);
  digitalWrite(solenoid_4,HIGH);
  digitalWrite(solenoid_5,HIGH);
  digitalWrite(solenoid_6,HIGH);
  digitalWrite(solenoid_7,HIGH);

}
void re()
{
  digitalWrite(solenoid_1,HIGH);
  digitalWrite(solenoid_2,HIGH);
  digitalWrite(solenoid_3,HIGH);
  digitalWrite(solenoid_4,HIGH);
  digitalWrite(solenoid_5,HIGH);
  digitalWrite(solenoid_6,HIGH);
  digitalWrite(solenoid_7,LOW);

}

void mi()
{
  digitalWrite(solenoid_1,HIGH);
  digitalWrite(solenoid_2,HIGH);
  digitalWrite(solenoid_3,HIGH);
  digitalWrite(solenoid_4,HIGH);
  digitalWrite(solenoid_5,HIGH);
  digitalWrite(solenoid_6,LOW);
  digitalWrite(solenoid_7,LOW);

}

void fa()
{
  digitalWrite(solenoid_1,HIGH);
  digitalWrite(solenoid_2,HIGH);
  digitalWrite(solenoid_3,HIGH);
  digitalWrite(solenoid_4,HIGH);
  digitalWrite(solenoid_5,LOW);
  digitalWrite(solenoid_6,HIGH);
  digitalWrite(solenoid_7,LOW);

}


void fad()
{
  digitalWrite(solenoid_1,HIGH);
  digitalWrite(solenoid_2,HIGH);
  digitalWrite(solenoid_3,HIGH);
  digitalWrite(solenoid_4,HIGH);
  digitalWrite(solenoid_5,HIGH);
  digitalWrite(solenoid_6,HIGH);
  digitalWrite(solenoid_7,LOW);

}

void sol()
{
  digitalWrite(solenoid_1,HIGH);
  digitalWrite(solenoid_2,HIGH);
  digitalWrite(solenoid_3,HIGH);
  digitalWrite(solenoid_4,LOW);
  digitalWrite(solenoid_5,LOW);
  digitalWrite(solenoid_6,LOW);
  digitalWrite(solenoid_7,LOW);

}

void la()
{
  digitalWrite(solenoid_1,HIGH);
  digitalWrite(solenoid_2,HIGH);
  digitalWrite(solenoid_3,LOW);
  digitalWrite(solenoid_4,LOW);
  digitalWrite(solenoid_5,LOW);
  digitalWrite(solenoid_6,LOW);
  digitalWrite(solenoid_7,LOW);
}

void sii()
{
  digitalWrite(solenoid_1,HIGH);
  digitalWrite(solenoid_2,LOW);
  digitalWrite(solenoid_3,HIGH);
  digitalWrite(solenoid_4,HIGH);
  digitalWrite(solenoid_5,LOW);
  digitalWrite(solenoid_6,LOW);
  digitalWrite(solenoid_7,LOW);
}


void si()
{
  digitalWrite(solenoid_1,HIGH);
  digitalWrite(solenoid_2,LOW);
  digitalWrite(solenoid_3,LOW);
  digitalWrite(solenoid_4,LOW);
  digitalWrite(solenoid_5,LOW);
  digitalWrite(solenoid_6,LOW);
  digitalWrite(solenoid_7,LOW);
}

void doo()
{
  digitalWrite(solenoid_1,LOW);
  digitalWrite(solenoid_2,HIGH);
  digitalWrite(solenoid_3,LOW);
  digitalWrite(solenoid_4,LOW);
  digitalWrite(solenoid_5,LOW);
  digitalWrite(solenoid_6,LOW);
  digitalWrite(solenoid_7,LOW);
}
